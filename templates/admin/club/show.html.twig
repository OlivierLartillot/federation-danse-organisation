{% extends 'base.html.twig' %}

{% block title %}Club{% endblock %}

{% block body %}
    <h1>Club</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ club.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ club.name }}</td>
            </tr>
            <tr>
                <th>Correspondents</th>
                <td>{{ club.correspondents }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ club.adress }}</td>
            </tr>
            <tr>
                <th>PhonePrimary</th>
                <td>{{ club.phonePrimary }}</td>
            </tr>
            <tr>
                <th>PhoneSecondary</th>
                <td>{{ club.phoneSecondary }}</td>
            </tr>
            <tr>
                <th>EmailPrimary</th>
                <td>{{ club.emailPrimary }}</td>
            </tr>
            <tr>
                <th>EmailSecondary</th>
                <td>{{ club.emailSecondary }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ club.fax }}</td>
            </tr>
            <tr>
                <th>WebSite</th>
                <td>{{ club.webSite }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_club_index') }}">back to list</a>

    <a href="{{ path('app_admin_club_edit', {'id': club.id}) }}">edit</a>

    {{ include('admin/club/_delete_form.html.twig') }}
{% endblock %}
