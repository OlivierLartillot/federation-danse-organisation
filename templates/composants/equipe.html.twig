    <!--==============================
    Team Area  
    ==============================-->
    <section class="space">
        <div class="container">
            <div class="title-area text-center">
                <span class="sub-title mt-n1">Equipe</span>
                <h2 class="sec-title">Dirigeants et Comité</h2>
            </div>
            <div class="row slider-shadow th-carousel" data-slide-show="4" data-lg-slide-show="3" data-md-slide-show="2" data-sm-slide-show="2" data-xs-slide-show="1" data-arrows="true">
                <!-- foreach Item -->
                {% for dirigeant in listeDirigeants %}
                    <div class="col-sm-6 col-lg-4">
                        <div class="th-team team-card">
                            <div class="team-img">
                                <img src="{{ dirigeant.photo ? vich_uploader_asset(dirigeant, 'imageFile') :  'assets/img/team/team_2_1.jpg'}} " alt="Team">
                            </div>
                            <div class="team-content">
                                <h3 class="team-title box-title"><a href="single-instructor.html">{{ dirigeant.name }}</a></h3>
                                <span class="team-desig">{{ dirigeant.organizationRole }}</span>
                            </div>
                            <div class="team-social">
                                {% if dirigeant.facebook %}
                                    <a href="https://facebook.com/{{dirigeant.facebook}}" target="_blank"><i class="fab fa-facebook-f"></i></a>
                                {% endif %}
                                {% if dirigeant.twitter %}
                                    <a href="https://twitter.com/{{dirigeant.twitter}}" target="_blank"><i class="fab fa-twitter"></i></a>
                                {% endif %}
                                {% if dirigeant.instagram %}
                                    <a href="https://instagram.com/{{dirigeant.instagram}}" target="_blank"><i class="fab fa-instagram"></i></a>
                                {% endif %}
                                {% if dirigeant.linkedin %}
                                    <a href="https://linkedin.com/{{dirigeant.linkedin}}" target="_blank"><i class="fab fa-linkedin-in"></i></a>
                                {% endif %}
                            </div>
                        </div>
                    </div>
                {% endfor %}
                <!-- END FOREACH -->

                <!-- Single Item -->
                {#                 
                    <div class="col-sm-6 col-lg-4">
                        <div class="th-team team-card">
